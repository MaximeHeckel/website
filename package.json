{
  "name": "website",
  "version": "1.0.0",
  "description": "Personal website",
  "main": "webpack.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "export ENV='DEV' && cp src/index.html index.html && cp -r src/img dist/img && webpack-dev-server --hot --progress --colors",
    "server": "npm run build:server && go run -v go/src/server/main.go -entry=./index.html -static=./",
    "start": "npm-run-all -p -r server dev",
    "build:frontenddev": "cp src/index.html index.html && cp -r src/img dist/img && webpack --progress --colors",
    "build:frontend": "export ENV='PROD' && cp src/index.html index.html && webpack --progress --colors && cp -r src/img dist/",
    "build:servermac": "go build -o server_mac go/src/server/main.go",
    "build:server": "CGO_ENABLED=0 GOOS=linux go build -ldflags \"-s\" -a -installsuffix cgo -o server go/src/server/main.go",
    "build": "npm run build:frontenddev && server"
  },
  "author": "Maxime Heckel",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.24.0",
    "eslint": "2.2.0",
    "eslint-config-airbnb": "6.0.1",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.0.0",
    "estraverse-fb": "^1.3.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "0.8.5",
    "lodash": "^4.16.2",
    "npm-run-all": "^3.1.0",
    "postcss": "5.0.15",
    "postcss-calc": "5.2.0",
    "postcss-cssnext": "2.4.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-import": "7.1.3",
    "postcss-loader": "0.7.0",
    "postcss-media-minmax": "^2.1.2",
    "postcss-nested": "1.0.0",
    "postcss-simple-vars": "1.2.0",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-modal": "^1.4.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "redux": "^3.5.2",
    "redux-actions": "^0.12.0",
    "redux-logger": "^2.6.1",
    "redux-promise-middleware": "^4.0.0"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015",
      "react"
    ]
  }
}
