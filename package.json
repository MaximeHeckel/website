{
  "name": "website",
  "version": "1.0.0",
  "description": "Personal website",
  "main": "webpack.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:servermac": "go build -o server_mac go/src/server/main.go",
    "build:server": "CGO_ENABLED=0 GOOS=linux go build -ldflags \"-s\" -a -installsuffix cgo -o server go/src/server/main.go",
    "start": "cp src/index.html dist/index.html && webpack-dev-server --hot --progress --colors",
    "build:frontend": "cp src/index.html dist/index.html && webpack --progress --colors"
  },
  "author": "Maxime Heckel",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-react": "^6.5.0",
    "eslint": "2.2.0",
    "eslint-config-airbnb": "6.0.1",
    "eslint-loader": "1.3.0",
    "eslint-plugin-react": "4.0.0",
    "estraverse-fb": "^1.3.1",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-redux": "^4.4.5",
    "react-router": "^2.7.0",
    "redux": "^3.5.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  }
}
